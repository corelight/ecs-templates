{
  "template": {
    "settings": {
      "index": {
        "mapping": {
          "total_fields.limit": 10000,
          "ignore_malformed": true
        }
      }
    },
    "mappings": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "string_fields": {
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 2048,
              "type": "keyword"
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "ignore_malformed": false,
          "type": "date"
        },
        "@version": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "message": {
          "norms": false,
          "type": "text"
        },
        "ecs": {
          "properties": {
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "tags": {
          "type": "keyword"
        },
        "error": {
          "properties": {
            "code": {
              "type": "keyword"
            },
            "id": {
              "type": "keyword"
            },
            "message": {
              "ignore_above": 32000,
              "type": "keyword",
              "fields": {
                "text": {
                  "type": "text"
                }
              }
            },
            "stack_trace": {
              "ignore_above": 32000,
              "type": "keyword",
              "fields": {
                "text": {
                  "type": "text"
                }
              }
            },
            "type": {
              "type": "keyword"
            }
          }
        },
        "event": {
          "properties": {
            "category": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "created": {
              "type": "date"
            },
            "dataset": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "duration": {
              "type": "long"
            },
            "end": {
              "type": "date"
            },
            "facility": {
              "type": "keyword"
            },
            "hash": {
              "type": "keyword"
            },
            "id": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "ingested": {
              "type": "date"
            },
            "kind": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "module": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "original": {
              "doc_values": false,
              "index": false,
              "type": "keyword"
            },
            "outcome": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "priority": {
              "type": "keyword"
            },
            "risk_score": {
              "type": "float"
            },
            "risk_score_norm": {
              "type": "float"
            },
            "severity": {
              "type": "keyword"
            },
            "start": {
              "type": "date"
            },
            "timezone": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        }
      }
    }
  },
  "version": 2023101701,
  "_meta": {
    "description": "Corelight ECS Templates. For technical issues, questions, and documentation see https://github.com/corelight/ecs-templates",
    "ecs_version": "8.10.0",
    "labels_corelight_ecs_version": "8.10.0.1"
  }
}
